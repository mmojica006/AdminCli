
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/layoutDeat.cshtml";
}

<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Notificación Individual</h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Basic Form Elements
                </div>
                <div class="panel-body">
                    <div class="row">
                        <div class="col-md-offset-2 col-md-6 col-md-offset-3">
                            <form role="form">
                                <div class="form-group">
                                    <label>Titulo</label>
                                    <input class="form-control">

                                </div>
                                <div class="form-group">
                                    <label>Cuerpo del mensaje</label>
                                    <textarea class="form-control" rows="3"></textarea>
                                </div>
                                <div class="form-group">
                                    <label>Seleccione el Cliente</label>
                                    <select class="form-control">
                                        <option>Seleccione...</option>
                                        <option>Moises Mojica </option>
                                        <option>Eduardo Perez</option>
                                        <option>Maria Teresa</option>
                                        <option>Amin Mojica</option>
                                    </select>
                                </div>                         
                                                                              
                                <button type="submit" class="btn btn-primary">Enviar</button>
                                <button type="reset" class="btn btn-default">Cancelar</button>
                            </form>
                        </div>
                        <!-- /.col-lg-6 (nested) -->
                   
                        <!-- /.col-lg-6 (nested) -->
                    </div>
                    <!-- /.row (nested) -->
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
</div>
<!-- /#page-wrapper -->
